        <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model $(arg robot_name) -x 5 -y 8 -z 0.041 " />
      </group>
      <group ns="bat_9">
        <arg name="robot_name" default="bat_9"/>
        <arg name="color" default="Purple"/>
        <param name="robot_description" command="$(find xacro)/xacro $(find bat_algo)/urdf/small_bot.xacro color:='$(arg color)' robot_name:='$(arg robot_name)'" />
        <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model $(arg robot_name) -x -2 -y 5 -z 0.041 " />
      </group>
      <group ns="bat_10">
        <arg name="robot_name" default="bat_10"/>
        <arg name="color" default="White"/>
        <param name="robot_description" command="$(find xacro)/xacro $(find bat_algo)/urdf/small_bot.xacro color:='$(arg color)' robot_name:='$(arg robot_name)'" />
        <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model $(arg robot_name) -x 8 -y 1 -z 0.041 " />
      </group>
  </group>
</launch>